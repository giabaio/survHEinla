RoxygenNote: 7.3.2
Biarch: true
Depends: 
    survHE,
    R (>= 4.1.0),
    dplyr
Imports: 
    flexsurv,
    INLA
Additional_repositories:
    https://inla.r-inla-download.org/R/stable/
Config/extra-repositories: 
    https://inla.r-universe.dev
SystemRequirements: GNU make
NeedsCompilation: yes
